    "import numpy as np\n",
    "import qutip as qt\n",
    "\n",
    "I = qt.qeye(2).full()\n",
    "X = qt.sigmax().full()\n",
    "Y = qt.sigmay().full()\n",
    "Z = qt.sigmaz().full()\n",
    "\n",
    "a = [I/2, X/2, Y/2, Z/2]\n",
    "\n",
    "np.save(\"rohan\", a)"
   ]
  }
